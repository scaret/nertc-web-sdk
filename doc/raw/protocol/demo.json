/**
 * 共有, 大小写是为了区分请求和响应
 * SID/sid : service id
 * CID/cid : cmd id
 * SER/ser : 协议序列号, 用于同步请求响应
 *
 * 请求
 * Q : 请求参数列表
 * t: 参数类型
 * v: 参数内容
 *
 * 返回
 * code: response code
 * r: 响应参数列表
 * 
 * PacketHeader
 * serial:serial id
 * len: packet length
 *
 * 其它
 * i: 参数在请求参数或返回对象中的序号, 0开始递增；
 */

// 登陆协议
{
    "SID": 2,
    "CID": 3,
    "SER": 11,
    "Q": [
        {
            "t": "property",
            "v": {
                // os 系统类型
                "4": "Windows XP",
                // sdkVersion 客户端版本
                "6": 1,
                // protocolVersion 协议版本号
                "9": 1,
                // deviceId 当前设备id, 随机uuid
                "13": "device_abc",
                // appKey app标示符
                "18": "45c6af3c98409b18a84451215d0bdd6e",
                // appAccount 帐号, 用于反查nim中uid登陆
                "19": "zlw2",
                // browser 浏览器
                "24": "chrome",
                // loginToken 登录的token
                "1000": "asdf"
            }
        }
    ]
}

// 登陆协议响应

{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": "docs_en/",
    "typeRoots": [
      "docs_en/"
    ],
    "types": [],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [ "docs_en"],
  "typedocOptions": {
    "entryPoints": [
      "docs_en/src/nertc.d.ts",
      "docs_en/src/client.d.ts",
      "docs_en/src/stream.d.ts",
      "docs_en/src/types.d.ts",
      "docs_en/src/constant.d.ts",
      "docs_en/src/browser.d.ts"
    ],
    "disableSources": true,
    "hideGenerator": true,
    "out": "dist/api_en"
  }
}
{
    "tags"      : {
        "allowUnknownTags" : true
    },
    "recurseDepth": 10,
    "source": {
        "include": [
            "src/im/api/nim",
            "src/im/api/base/avSignal.js",
            "src/im/api/chatroom",
            "src/im/model",
            "src/netcall/module/pcagent",
            "src/netcall/api-adapter/webrtc.js",
            "src/netcall/api-adapter/nrtc.js",
            "src/netcall/api-adapter/baseAdapter.js",
            "src/netcall/api-adapter/abstractAdapter.js",
            "src/netcall/constant/event",
            "src/netcall/constant/error",
            "src/entry/whiteboard.js",
            "src/whiteboard",
            "src/netcall-weixin/index.js",
            "src/netcall-weixin/api/rtc.js",
            "src/netcall-weixin/api/nrtc.js",
            "doc/api/stream.js",
            "doc/api/client.js",
            "doc/api/webrtc2.js",
            "src/util/index.js",
            "src/util/blob.js",
            "src/util/support.js",
            "src/util/logger.js",
            "doc/api/index.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins"   : ["plugins/markdown"],
    "templates" : {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles" : false
        },
        "applicationName"       : "云信Web SDK API文档",
        "disqus": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "云信Web SDK API文档",
            "description": "云信Web SDK API文档",
            "keyword": "云信Web SDK API文档"
        },
        "linenums": false
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "sort": false
}

{
    "tags"      : {
        "allowUnknownTags" : true
    },
    "recurseDepth": 10,
    "source": {
        "include": [
            "doc/api/stream.js",
            "doc/api/client.js",
            "doc/api/webrtc2.js",
            "doc/api/index.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins"   : ["plugins/markdown"],
    "templates" : {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles" : false
        },
        "applicationName"       : "云信Web SDK API文档",
        "disqus": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "云信Web SDK API文档",
            "description": "云信Web SDK API文档",
            "keyword": "云信Web SDK API文档"
        },
        "linenums": false
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "sort": false
}

{
  "name": "nimWebSdkTester",
  "version": "1.0.0",
  "private": true,
  "description": "file upload",
  "main": "app.js",
  "scripts": {
    "env": "env",
    "clean": "run-p -lns clean:*",
    "clean:css": "rimraf dist/css",
    "clean:js": "rimraf dist/js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-test": "webpack --config build/webpack.test.config.js",
    "unit": "karma start build/karma.unit.config.js",
    "lint": "eslint --config build/eslint.yaml --ignore-path build/eslintignore --color . ; exit 0",
    "prebuild:css": "npm run --silent clean:css",
    "prebuild:js": "npm run --silent clean:js",
    "build": "run-p -lns build:js build:css",
    "build:favicon": "node build/favicon.js",
    "build:css": "postcss --config build/postcss.js",
    "build:js": "webpack --config build/webpack.config.js",
    "build:js:stats": "npm run build:js -- --profile --json > dist/webpack-stats.json",
    "watch": "run-p watch:*",
    "watch:css": "npm run build:css --silent -- --watch",
    "watch:js": "npm run build:js --silent -- --watch",
    "watch:lint": "watch 'npm run lint' -du --interval=0.25 --ignoreDirectoryPattern='/third|submodule|dist|node_modules/i' --filter=build/watchFilter.eslint.js",
    "dev": "npm run watch:lint",
    "prod": "NODE_ENV=production npm run build:js",
    "dist": "npm run build:js && npm run prod",
    "server": "puer & npm run dev",
    "archive": "git archive --format zip -o dist/regular-strap.zip master src dist",
    "prepub": "npm run dist && git add --all && git commit -m 'pre-publish' && npm run archive",
    "foo": "NODE_ENV=production node build/foo.js",
    "node": "node app.js",
    "py": "cd python && python manage.py runserver_plus --cert server.crt 0.0.0.0:3000",
    "https-server": "badcert --no-reload web",
    "http-server": "http-server --p 8082"
  },
  "author": "zyy",
  "license": "ISC",
  "dependencies": {
    "badcert": "^1.8.0",
    "bootstrap": "4.0.0-alpha.6",
    "date-fns": "^1.27.0",
    "http-server": "^0.12.3",
    "lodash": "^4.17.4",
    "vue": "^2.1.10"
  },
  "devDependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.12.3",
    "font-awesome": "^4.6.3",
    "javascript-natural-sort": "^0.7.1",
    "multer": "^0.1.8",
    "prettyjson": "^1.1.3",
    "purecss": "^0.6.0",
    "weinre": "^2.0.0-pre-I0Z7U9OV"
  }
}
